<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.QuickLTEForce4G5G" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/_colorAccent</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.ScheduleTime.BackgroundSplash" parent="">
        <item name="android:background">@color/_colorAccent</item>
    </style>
    <style name="Theme.ScheduleTime.custom_background" parent="">
        <item name="android:background">@drawable/custom_background_night</item>
    </style>
    <style name="Theme.ScheduleTime.Background" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@color/colorAccent</item>
    </style>
    <style name="Theme.ScheduleTime.LinearLayout.ListItem.CircleClickable1" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_night_</item>
    </style>
    <style name="Theme.ScheduleTime.LinearLayout.ListItem.CircleClickable2" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_night</item>
    </style>
    <style name="Theme.ScheduleTime.TextView.Clickable" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_night</item>
        <item name="android:textColor">@color/red</item>
    </style>
    <style name="Theme.ScheduleTime.TextView.CircleClickable2" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_night</item>
    </style>
</resources>